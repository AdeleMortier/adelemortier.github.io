@article{Katzir2015, title={Economy of structure and information: Oddness, questions, and answers}, volume={19}, url={https://ojs.ub.uni-konstanz.de/sub/index.php/sub/article/view/236}, DOI={10.18148/sub/2015.v19i0.236}, abstractNote={&amp;lt;p&amp;gt;We examine two conflicting perspectives on oddness: Magri (2009, 2011)’s theory, which derives oddness from blind inferences that clash with common knowledge, and Spector (2014)’s theory that derives oddness from trivial alternatives. Building on these works, we offer a third alternative, one that relies on a discourse condition that says that a good assertion is one that provides a good answer to a good question. A remaining difficulty is the persistence of oddness when the relevant sentences are embedded in environments that are predicted to satisfy the proposed appropriateness conditions.&amp;lt;/p&amp;gt;}, journal={Proceedings of Sinn und Bedeutung}, author={Katzir, Roni and Singh, Raj}, year={2015}, month={5}, pages={322–339} }

